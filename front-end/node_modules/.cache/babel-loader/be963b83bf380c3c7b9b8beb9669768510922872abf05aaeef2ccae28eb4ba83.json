{"ast":null,"code":"import axios from 'axios';\nimport ShoppingCartList from '@/components/ShoppingCartList.vue';\nexport default {\n  name: \"ShoppingCartPage\",\n  prop: ['user'],\n  components: {\n    ShoppingCartList\n  },\n  data() {\n    return {\n      cartItems: []\n    };\n  },\n  watch: {\n    async user(newUserValue) {\n      if (newUserValue) {\n        const cartResponse = await axios.get(`/api/users/${newUserValue.uid}/cart`);\n        const cartItems = cartResponse.data;\n        this.cartItems = cartItems;\n      }\n    }\n  },\n  methods: {\n    async removeFromCart(productId) {\n      const response = await axios.delete(`http://127.0.0.1:8000/api/users/$/cart/${productId}`);\n      const updatedCart = response.data;\n      this.cartItems = updatedCart;\n    }\n  },\n  async created() {\n    const response = await axios.get(`/api/users/12345/cart`);\n    const cartItems = response.data;\n    this.cartItems = cartItems;\n  }\n};","map":{"version":3,"names":["axios","ShoppingCartList","name","prop","components","data","cartItems","watch","user","newUserValue","cartResponse","get","uid","methods","removeFromCart","productId","response","delete","updatedCart","created"],"sources":["C:\\Users\\User\\OneDrive\\VALENTINA\\Escritorio\\vue\\front-end\\src\\pages\\ShoppingCartPage.vue"],"sourcesContent":["<template>\r\n    <h1> Shopping cart</h1>\r\n    <div v-if=\"cartItems. length > 0\">\r\n        <ShoppingCartList @remove-from-cart=\"removeFromCart($event)\" :products=\"cartItems\" />\r\n        <button class=\"checkout-button\">Checkout</button>\r\n    </div>\r\n    <div v-if=\"cartItems.length === 0\"> \r\n        You have no items in your cart\r\n    </div>\r\n</template>\r\n\r\n\r\n\r\n<script>\r\n\r\nimport axios from 'axios';\r\nimport ShoppingCartList from '@/components/ShoppingCartList.vue';\r\n\r\nexport default {\r\n    name: \"ShoppingCartPage\",\r\n    prop: ['user'],\r\n    components: {\r\n        ShoppingCartList,\r\n    },\r\n    data() {\r\n        return {\r\n            cartItems: [],\r\n        }\r\n    },\r\n    watch : {\r\n        async user(newUserValue){\r\n            if (newUserValue) {\r\n                const cartResponse = await axios.get(`/api/users/${newUserValue.uid}/cart`);\r\n                const cartItems = cartResponse.data;\r\n                this.cartItems = cartItems;\r\n            }\r\n        }\r\n    },\r\n    methods: {\r\n        async removeFromCart(productId) {\r\n            const response = await axios.delete(`http://127.0.0.1:8000/api/users/$/cart/${productId}`);\r\n            const updatedCart = response.data;\r\n            this.cartItems = updatedCart;\r\n        },\r\n    },\r\n    async created() {\r\n        const response = await axios.get(`/api/users/12345/cart`);\r\n        const cartItems = response.data;\r\n        this.cartItems = cartItems;\r\n    }\r\n    \r\n}\r\n</script>"],"mappings":"AAeA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,gBAAe,MAAO,mCAAmC;AAEhE,eAAe;EACXC,IAAI,EAAE,kBAAkB;EACxBC,IAAI,EAAE,CAAC,MAAM,CAAC;EACdC,UAAU,EAAE;IACRH;EACJ,CAAC;EACDI,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,SAAS,EAAE;IACf;EACJ,CAAC;EACDC,KAAI,EAAI;IACJ,MAAMC,IAAIA,CAACC,YAAY,EAAC;MACpB,IAAIA,YAAY,EAAE;QACd,MAAMC,YAAW,GAAI,MAAMV,KAAK,CAACW,GAAG,CAAC,cAAcF,YAAY,CAACG,GAAG,OAAO,CAAC;QAC3E,MAAMN,SAAQ,GAAII,YAAY,CAACL,IAAI;QACnC,IAAI,CAACC,SAAQ,GAAIA,SAAS;MAC9B;IACJ;EACJ,CAAC;EACDO,OAAO,EAAE;IACL,MAAMC,cAAcA,CAACC,SAAS,EAAE;MAC5B,MAAMC,QAAO,GAAI,MAAMhB,KAAK,CAACiB,MAAM,CAAC,0CAA0CF,SAAS,EAAE,CAAC;MAC1F,MAAMG,WAAU,GAAIF,QAAQ,CAACX,IAAI;MACjC,IAAI,CAACC,SAAQ,GAAIY,WAAW;IAChC;EACJ,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACZ,MAAMH,QAAO,GAAI,MAAMhB,KAAK,CAACW,GAAG,CAAC,uBAAuB,CAAC;IACzD,MAAML,SAAQ,GAAIU,QAAQ,CAACX,IAAI;IAC/B,IAAI,CAACC,SAAQ,GAAIA,SAAS;EAC9B;AAEJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}